{
  "name": "app-wrapper",
  "version": "0.1.0",
  "description": "An elegant and scalable solution for testing with supertest",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf lib",
    "copy:assets": "rsync -a --exclude .eslintrc src/assets lib",
    "compile": "babel src -d lib --ignore src/assets",
    "build": "yarn compile && yarn copy:assets",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mutaimwiti/app-wrapper.git"
  },
  "keywords": [
    "cli",
    "tdd",
    "node",
    "auth",
    "testing",
    "express",
    "wrapper",
    "supertest",
    "auth-wrapper",
    "app-wrapper"
  ],
  "author": "Mutai Mwiti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mutaimwiti/app-wrapper/issues"
  },
  "homepage": "https://gitlab.com/mutaimwiti/app-wrapper#readme",
  "bin": {
    "app-wrapper": "./index.js"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "inquirer": "^7.0.0",
    "is-invalid-path": "^1.0.2",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "1.19.1",
    "rimraf": "^3.0.0"
  }
}
